            <Insets left="5.0" right="5.0" />
         </GridPane.margin>
      </JFXTextField>
      <JFXTextField fx:id="txtSectorPertenece" labelFloat="true" layoutX="10.0" layoutY="274.0" onMouseClicked="#actBuscarSector" prefHeight="50.0" promptText="Distrito" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="5.0" right="5.0" />
         </GridPane.margin>
      </JFXTextField>
      <Label prefHeight="91.0" prefWidth="529.0" text="Registro y edicción de información">
         <GridPane.margin>
            <Insets left="5.0" right="5.0" />
         </GridPane.margin>
      </Label>
      <JFXComboBox fx:id="cbTipo" prefHeight="50.0" prefWidth="557.0" promptText="Tipo de Unidad de distrito" GridPane.rowIndex="3">
