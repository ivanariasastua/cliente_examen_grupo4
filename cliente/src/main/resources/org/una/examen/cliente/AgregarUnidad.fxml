   <children>
      <JFXTextField fx:id="txtNombre" labelFloat="true" prefHeight="50.0" promptText="Nombre" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="5.0" right="5.0" />
         </GridPane.margin>
      </JFXTextField>
      <JFXTextField fx:id="txtCodigo" labelFloat="true" layoutX="10.0" layoutY="80.0" prefHeight="50.0" promptText="Codigo" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="5.0" right="5.0" />
         </GridPane.margin>
      </JFXTextField>
      <JFXButton onAction="#actGuardar" prefHeight="50.0" prefWidth="510.0" text="Guardar" GridPane.rowIndex="7">
         <GridPane.margin>
            <Insets left="5.0" right="5.0" />
         </GridPane.margin>
         <styleClass>
            <String fx:value="botonTerciario" />
            <String fx:value="boton" />
         </styleClass>
      </JFXButton>
      <JFXTextField fx:id="txtPoblacion" labelFloat="true" layoutX="10.0" layoutY="134.0" prefHeight="50.0" promptText="Población" GridPane.rowIndex="5">
         <GridPane.margin>
            <Insets left="5.0" right="5.0" />
         </GridPane.margin>
      </JFXTextField>
      <JFXTextField fx:id="txtArea" labelFloat="true" layoutX="10.0" layoutY="233.0" prefHeight="50.0" promptText="Área" GridPane.rowIndex="6">
